{
  "name": "ch2plan",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Evan <github@erosson.org>",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "prepare": "elm-package install --yes",
    "tf:init": "terraform init",
    "tf:plan": "terraform plan",
    "tf:apply": "terraform apply -auto-approve",
    "ch2export": "scripty",
    "export:chars": "scripty",
    "dev": "cp CHANGELOG.md ./assets && elm-live ./src/Main.elm --output=./assets/elm.js --dir=./assets --open",
    "build": "rm -rf ./dist && mkdir -p ./dist && cp CHANGELOG.md ./dist && rsync -auv ./assets/ ./dist/ && elm-make ./src/Main.elm --output=./dist/elm.js",
    "test": "echo 'no tests yet'",
    "start": "yarn build && serve ./dist"
  },
  "//optionalDependencies": "because travis fails to install me, but doesn't actually need me",
  "optionalDependencies": {
    "terraform-npm": "0.2.6"
  },
  "dependencies": {},
  "devDependencies": {
    "elm": "^0.18.0",
    "elm-live": "^2.7.5",
    "scripty": "^1.7.2"
  }
}
